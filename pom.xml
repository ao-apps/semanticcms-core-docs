				</dependency>
				<dependency>
					<groupId>com.semanticcms</groupId><artifactId>semanticcms-view-what-links-here</artifactId>
				</dependency>
			</dependencies>
		</profile>
		<profile>
			<id>release</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId><artifactId>maven-war-plugin</artifactId>
						<configuration>
							<overlays>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-model</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/model/apidocs</targetPath>
								</overlay>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-servlet</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/servlet/apidocs</targetPath>
								</overlay>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-sitemap</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/sitemap/apidocs</targetPath>
								</overlay>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-style</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/style/apidocs</targetPath>
								</overlay>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-taglib</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/taglib/apidocs</targetPath>
								</overlay>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-theme-base</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/theme-base/apidocs</targetPath>
								</overlay>
								<overlay>
									<groupId>com.semanticcms</groupId><artifactId>semanticcms-core-view-content</artifactId>
									<type>jar</type>
									<classifier>javadoc</classifier>
									<targetPath>core/view-content/apidocs</targetPath>
								</overlay>
							</overlays>
						</configuration>
					</plugin>
				</plugins>
			</build>

			<dependencies>
